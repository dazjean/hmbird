{
  "name": "hmbird-ssr",
  "version": "2.0.0",
  "description": "HMbird零成本升级react服务端渲染方案,支持静态导出,cdn部署,前后端同构,自定义路由前缀,自定义webpack,服务端渲染缓存",
  "main": "webpack.config.js",
  "scripts": {
    "output": "node hmbird/lib/build.js --cate",
    "dev": "webpack-dev-server  --progress --colors --cate",
    "build": "webpack --config webpack.config_build.js -p  --cate",
    "build:server": "babel server --out-dir dist/server  --copy-files",
    "start": "node server/start.js",
    "arthur-start": "pm2 start server/app.js --name=hmbirdSsrService",
    "arthur-stop": "pm2 stop hmbirdSsrService",
    "arthur": "npm run build:server",
    "lint:server": "eslint server",
    "lint": "eslint src/pages --fix && eslint src/components --fix && eslint hmbird/src --fix"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:dazjean/hmbird-template.git"
  },
  "author": "zhangjiajia",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.20.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.5.2",
    "copyfiles": "^2.1.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.7.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-react-hooks": "^1.0.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mockjs": "^1.0.1-beta3",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^4.10.0",
    "open-browser-webpack-plugin": "0.0.5",
    "optimist": "^0.6.1",
    "postcss": "^7.0.18",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2",
    "koa-send": "^5.0.0",
    "watchpack": "^1.6.0",
    "react-router-dom": "^4.3.1",
    "react-dom": "^16.8.1",
