    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.6.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.3.1"
  }
}
